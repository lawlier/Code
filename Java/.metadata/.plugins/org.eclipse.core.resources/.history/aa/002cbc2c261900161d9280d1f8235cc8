package year_2016.polygongame;

public class Node {
	private float x, y;
	private int value;
	
	public Node(float x, float y) {
		// TODO Auto-generated constructor stub
		this.x = x;
		this.y = y;
	}
	public void setValue(int value){
		this.value = value;
	}
	public int getValue(){
		return value;
	}
	public float getX(){
		return x;
	}
	public float getY(){
		return y;
	}
}
