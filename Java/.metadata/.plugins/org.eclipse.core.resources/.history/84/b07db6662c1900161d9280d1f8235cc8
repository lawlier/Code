package year_2016.polygongame;

public class Edge {
	private Node p1, p2;
	private int index;
	public Edge(Node p1, Node p2,int index){
		this.p1 = p1;
		this.p2 = p2;
		this.index = index;
	}
	public void setNode1(Node p){
		this.p1 = p;
	}
	public void setNode2(Node p){
		this.p2 = p;
	}
	public Node getNode1(){
		return p1;
	}
	public Node getNode2(){
		return p2;
	}
	
}
