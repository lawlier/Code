<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from HelloService WSDL service -->

  <service name="Hello" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://lawliex.cn"/>
      <parameter name="wsdlServiceElement" value="HelloService"/>
      <parameter name="schemaQualified" value="http://lawliex.cn"/>
      <parameter name="wsdlServicePort" value="Hello"/>
      <parameter name="className" value="cn.lawliex.HelloSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Hello"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getName" qname="operNS:getName" xmlns:operNS="http://lawliex.cn" returnQName="retNS:getNameReturn" xmlns:retNS="http://lawliex.cn" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:ans" xmlns:pns="http://lawliex.cn" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getName"/>

  </service>
</deployment>
